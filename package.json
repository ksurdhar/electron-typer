{
  "name": "hackathon",
  "productName": "hackathon",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Kiran Surdhar",
    "email": "kiran.surdhar@workday.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "hackathon"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.jsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "electron-settings": "^3.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "html-pdf": "^2.2.0",
    "lorem-ipsum": "^2.0.3",
    "node-quill-converter": "^0.3.2",
    "quill": "^1.3.7",
    "react-modal": "^3.11.1",
    "react-quill": "^1.3.3",
    "react-select": "^3.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@electron-forge/cli": "6.0.0-beta.46",
    "@electron-forge/maker-deb": "6.0.0-beta.46",
    "@electron-forge/maker-rpm": "6.0.0-beta.46",
    "@electron-forge/maker-squirrel": "6.0.0-beta.46",
    "@electron-forge/maker-zip": "6.0.0-beta.46",
    "@electron-forge/plugin-webpack": "6.0.0-beta.46",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "electron": "7.1.3",
    "node-loader": "^0.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "style-loader": "^0.23.1"
  }
}
